def fibonacci(n):
    fib_sequence = []
    a, b = 0, 1
    while len(fib_sequence) < n:
        fib_sequence.append(a)
        a, b = b, a + b
    return fib_sequence
    n = int(input("Enter the number of Fibonacci numbers to generate: "))
    fib_sequence = fibonacci(n)
print("Fibonacci Sequence:")
print(fib_sequence)
